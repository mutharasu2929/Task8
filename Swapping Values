package Task8;

public class SwappingNumbers {

	public static void main(String[] args) {
		int x=10;
		int y=25;
		System.out.println("Before Swapping value of x is" + x + "value of y is" +y);
		
		int temp=x;
		x=y;
		y=temp;
		System.out.println("After swapping the value of x is " + x + " and the value of y is " + y);
		
	}

}
